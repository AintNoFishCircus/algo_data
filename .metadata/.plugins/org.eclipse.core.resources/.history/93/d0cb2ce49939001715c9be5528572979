package grundlegendeDatenstrukturen;
import grundlegendeDatenstrukturen.DList;

public class Queue extends DList{
	
	DList queue;
	
	public Queue(){
		queue = new DList();
	}
	
	public void enter(Object o){
		queue.addLast(o);
	}

	public Object leave(){
		return queue.removeFirst();
	}
	
	public Object front(){
		return queue.getFirst();
	}

	
}
