package sorts;

import java.util.ArrayList;

public class Sort {

	public static void insertionSort(ArrayList<Integer> array){
		for(int i = 1; i < array.size(); i ++){
			int temp = array.get(i);
			
			for(int j = i -1; j >= 0; i--){
				if(array.get(j) > temp){
				array.set(i, array.get(j));
				array.set(j, temp);
				}
			}
		}
	}
}
